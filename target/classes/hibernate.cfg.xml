<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
"-//Hibernate/Hibernate Configuration DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
 
<hibernate-configuration>
  <session-factory>
    <property name="connection.driver_class">com.microsoft.sqlserver.jdbc.SQLServerDriver</property>
    <property name="hibernate.dialect">org.hibernate.dialect.SQLServerDialect</property>
    <property name="show_sql">false</property>
    <property name="format_sql">true</property>
    <mapping class="polyu.learn.bbssoadfs.operation.sis.SISIntgrLog" />
    <mapping class="polyu.learn.bbssoadfs.operation.ssoadfs.ADFSUser" />
    <mapping class="polyu.learn.bbssoadfs.operation.cadn.CADN" />
    <mapping class="polyu.learn.bbssoadfs.operation.cadn.ADFSLog" />
    <mapping class="polyu.learn.bbssoadfs.operation.cadn.ADFSLogUpdateWrapper" />
    <mapping class="polyu.learn.bbssoadfs.operation.cadn.ADFSLogDeleteWrapper" />
    <mapping class="polyu.learn.bbssoadfs.operation.cadn.ADFSLogDelete" />
    </session-factory>
</hibernate-configuration>